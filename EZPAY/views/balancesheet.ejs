<script src="https://cdn.datatables.net/buttons/1.6.5/js/buttons.colVis.min.js"  crossorigin="anonymous"></script>
<script>
$(document).ready(function() {
    var table = $('#example').DataTable({
        lengthChange: false,
        
    } );
 
    table.buttons().container()
        .appendTo( '#example_wrapper .col-md-6:eq(0)' );
} );
</script>
</body>
