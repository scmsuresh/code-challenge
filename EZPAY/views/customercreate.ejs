.bg-light-red {
    color: #f95d6a;
    padding: 3px 15px;
    background: #f5b0b6;
    border-radius: 25px;
    font-size: 14px;
}
.card {
    border: none;
    margin-bottom: 2rem;
    -webkit-box-shadow: 0 4px 24px 0 rgba(34,41,47,.1);
    box-shadow: 0 4px 24px 0 rgba(34,41,47,.1);
}
